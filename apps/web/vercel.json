{
    "rewrites": [{ "source": "/apps/web/(.*)", "destination": "/apps/web/app/$1" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": { "AccessControlAllowOrigin": "*" }
        }
    ]
}
