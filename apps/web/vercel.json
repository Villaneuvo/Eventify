{
    "routes": [
        {
            "src": "/apps/web/(.*)",
            "dest": "/apps/web/app/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/app.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": { "Access-Control-Allow-Origin": "*" }
        }
    ]

}
